{
  "name": "terminal-devtools",
  "version": "1.0.1-rc1",
  "bin": {
    "devtools": "./cmd.js"
  },
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir compiled/ --source-maps",
    "lint": "spacey-standard",
    "watch": "npm-watch"
  },
  "spacey-standard": {
    "parser": "babel-eslint",
    "ignore": [
      "compiled/**",
      "examples/**"
    ]
  },
  "watch": {
    "build": "src/**/*.js"
  },
  "author": "David Mark Clements",
  "license": "MIT",
  "dependencies": {
    "babel": "^6.3.13",
    "babel-cli": "^6.3.17",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "blessed": "^0.1.81",
    "lsof": "^0.1.0",
    "minimist": "^1.2.0",
    "react": "^0.14.3",
    "react-blessed": "davidmarkclements/react-blessed#patches",
    "react-functional": "^1.2.0",
    "react-redux": "^4.0.1",
    "redux": "^3.0.5",
    "redux-logger": "^2.3.1",
    "redux-thunk": "^1.0.2",
    "source-map-support": "^0.4.0",
    "v8-debug": "^0.7.0",
    "yadc": "0.0.1"
  },
  "description": "Terminal based Devtools for Node",
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta6",
    "npm-watch": "0.0.1",
    "spacey-standard": "^1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nearform/terminal-devtools.git"
  },
  "bugs": {
    "url": "https://github.com/nearform/terminal-devtools/issues"
  },
  "homepage": "https://github.com/nearform/terminal-devtools#readme"
}
